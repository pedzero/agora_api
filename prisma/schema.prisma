generator client {
  provider = "prisma-client-js"
  output   = "./generated"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model User {
  id             Int      @id @default(autoincrement())
  name           String
  email          String   @unique
  username       String   @unique
  passwordHash   String
  profilePicture String?  
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
